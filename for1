package dongüü;

import java.util.Scanner;

public class for1 {

		public static void main(String [] args)	{
		Scanner giris=new Scanner (System.in);
		System.out.println("N gir: ");
		int n = giris.nextInt();

        for (int i = 1; i <= n; i++) {
            System.out.println(i + " çarpım tablosu:");
            for (int j = 1; j <= 10; j++) {
                System.out.println(i + " x " + j + " = " + (i * j));
            }
   
        }
    }
}
